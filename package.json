{
  "name": "generate-metadata",
  "version": "0.0.9",
  "description": "generate metadata",
  "keywords": [
    "generate",
    "metadata"
  ],
  "homepage": "https://github.com/ludicroushq/generate-metadata#readme",
  "bugs": {
    "url": "https://github.com/ludicroushq/generate-metadata/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ludicroushq/generate-metadata.git"
  },
  "license": "MIT",
  "author": "ludicrous",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./next": {
      "types": "./dist/next.d.ts",
      "require": "./dist/next.cjs",
      "import": "./dist/next.js"
    },
    "./tanstack-start": {
      "types": "./dist/tanstack-start.d.ts",
      "require": "./dist/tanstack-start.cjs",
      "import": "./dist/tanstack-start.js"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "wireit",
    "dev": "wireit",
    "dev:local": "wireit",
    "generate:local": "wireit",
    "generate:production": "wireit",
    "prepare": "wireit",
    "release": "wireit",
    "test": "wireit",
    "test:ci": "wireit"
  },
  "dependencies": {
    "debug": "^4.4.1",
    "es-toolkit": "^1.39.9",
    "generate-metadata": "https://pkg.pr.new/ludicroushq/generate-metadata@3386b47",
    "hono": "^4.8.5",
    "immer": "^10.1.1",
    "normalize-url": "^8.0.2",
    "openapi-fetch": "^0.14.0",
    "ts-pattern": "^5.7.1",
    "wireit": "^0.14.12",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.0",
    "@changesets/cli": "^2.29.5",
    "@types/debug": "^4.1.12",
    "@types/node": "^24.0.10",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "concurrently": "^9.2.0",
    "dotenv": "^17.1.0",
    "jiti": "^2.4.2",
    "lefthook": "^1.12.3",
    "openapi-typescript": "^7.8.0",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "ultracite": "5.2.2",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "@tanstack/react-start": ">=1.0.0",
    "next": ">=13.0.0"
  },
  "peerDependenciesMeta": {
    "next": {
      "optional": true
    },
    "@tanstack/react-start": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "wireit": {
    "prepare": {
      "command": "git rev-parse --git-dir > /dev/null 2>&1 && lefthook install || true"
    },
    "build": {
      "dependencies": [
        "pkg:build"
      ]
    },
    "dev": {
      "dependencies": [
        "pkg:dev"
      ]
    },
    "dev:local": {
      "dependencies": [
        "pkg:dev:local"
      ]
    },
    "generate:local": {
      "command": "openapi-typescript http://localhost:3000/api/openapi/spec.json -o ./src/__generated__/api.ts"
    },
    "generate:production": {
      "command": "openapi-typescript https://www.generate-metadata.com/api/openapi/spec.json -o ./src/__generated__/api.ts"
    },
    "pkg:build": {
      "command": "tsup",
      "dependencies": [
        "generate:production"
      ]
    },
    "pkg:dev": {
      "command": "tsup --watch",
      "dependencies": [
        "generate:production"
      ],
      "service": true
    },
    "pkg:dev:local": {
      "command": "tsup --watch",
      "env": {
        "GENERATE_METADATA_NODE_ENV": "local"
      },
      "service": true
    },
    "release": {
      "command": "changeset publish",
      "dependencies": [
        "pkg:build"
      ]
    },
    "test": {
      "dependencies": [
        "ultracite:format",
        "typescript:check",
        "vitest:run"
      ]
    },
    "test:ci": {
      "dependencies": [
        "ultracite:lint",
        "typescript:check",
        "vitest:run"
      ]
    },
    "typescript:check": {
      "command": "tsc --noEmit"
    },
    "ultracite:format": {
      "command": "ultracite format --unsafe"
    },
    "ultracite:lint": {
      "command": "ultracite lint"
    },
    "vitest:run": {
      "command": "vitest"
    }
  }
}
